    "fastify": "^4.26.2",
    "pino": "^9.2.0",
    "pino-loki": "^2.2.1",
    "pino-pretty": "^11.0.0",
    "ua-parser-js": "^1.0.37",
    "undici": "^6.10.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@nodesecure/eslint-config": "^1.9.0",
    "@types/node": "^20.11.30",
    "c8": "^10.1.2",
    "glob": "^10.3.10",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3"
